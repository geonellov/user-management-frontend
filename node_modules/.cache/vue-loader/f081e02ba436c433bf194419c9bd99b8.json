ode = true\n    }\n  },\n  data() {\n    return {\n      editMode:false,\n      userID: \"\",\n      userInfo:{\n        firstName: \"\",\n        lastName: \"\",\n        email: \"\"\n      }\n    }\n  },\n  methods: {\n    addUser () {\n      let userData  = this.userInfo\n      this.$store.dispatch('addUser', {userData})\n      this.clearForm()\n    },\n    editUser () {\n      let userData  = this.userInfo\n      userData['userID'] = this.userID\n      this.$store.dispatch('editUser', {userData})\n      this.clearForm()\n    },\n    cancelUpdate(){\n      this.clearForm()\n      this.editMode = false\n    },\n    deleteUser(){\n      let ID = this.userID\n      this.$store.dispatch('deleteUser', {ID})\n      this.editMode = false\n      this.clearForm()\n    },\n    clearForm(){\n      this.userID=\"\"\n      this.userInfo.firstName= \"\"\n      this.userInfo.lastName = \"\"\n      this.userInfo.email = \"\"\n    }\n  }\n}\n</script>"]}]}