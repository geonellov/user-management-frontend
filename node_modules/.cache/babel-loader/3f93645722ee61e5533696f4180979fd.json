},{userData}){\n      apolloProvider.defaultClient.mutate({\n        mutation: editUser,\n        variables: {\n          id: state.userID,\n          firstName: userData.firstName,\n          lastName: userData.lastName,\n          email: userData.email\n        }\n      }).then((data) => {\n          dispatch('getUsers')\n          console.log(data)\n      }).catch((error) => {\n        console.error(error)\n      })\n    },\n    deleteUser({dispatch},{userID}){\n      console.log(userID)\n      apolloProvider.defaultClient.mutate({\n        mutation: deleteUser,\n        variables: {\n          id: userID\n        }\n      }).then((data) => {\n        dispatch('getUsers')\n        console.log(data)\n      }).catch((error) => {\n        console.error(error)\n      })\n    },\n\n  },\n  modules: {\n  }\n})\n"]}]}